

class Box {
   double width;
   double height;
   double depth;
   
   void velume () {
	    System.out.print("The velume is : ");
	    system.out.println(width * height * depth);
   }
}

class BoxDemo {
	public static void main(String[] args) {
		Box myBox = new Box () , myBox2 = new Box() ;
		double vol , vol2 ; //volume  (obem)
		
		
		myBox.width = 100;
		myBox.height = 20;
		myBox.depth = 15;
		
		myBox2.width = 10;
		myBox2.height = 20;
		myBox2.depth = 5;
		
		myBox.velume();
		myBox2.velume();
			
	}

}
