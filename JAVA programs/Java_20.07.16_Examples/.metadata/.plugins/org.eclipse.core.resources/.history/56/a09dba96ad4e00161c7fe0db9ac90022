
class AutoBox5 {

	public static void main(String[] args) {
		Boolean b = false ;
		if(b){
			System.out.println("boolean b is true !!!");
		}else System.out.println("FALSE!!!");
		
        Character x  = 'c';
        
        char h1 = x;
        
        System.out.println("h1 : " + h1);
		
	}

}
