
public class ClassReturning {
   public static void main(String args[]){
	   Test2 ob1 = new Test2 (5);
	   System.out.println(" " + ob1.increaseBy10());
   }
}

class Test2 {
	int a;
	
	Test2 (int i) {
		a = i;
	}
	
	Test2 increaseBy10 () {
		Test2 temp = new Test2 (a + 10);
		System.out.println("a  : " + a);
		return temp;
	}
}