

class Box {
   double width;
   double height;
   double depth;
   
   double velume () {
	   
	   return  width * height * depth;
	     
   }
}

class BoxDemo {
	public static void main(String[] args) {
		Box myBox = new Box () , myBox2 = new Box() ;
		
		myBox.width = 100;
		myBox.height = 20;
		myBox.depth = 15;
		
		myBox2.width = 10;
		myBox2.height = 20;
		myBox2.depth = 5;
		
		System.out.println("The velume is : " + myBox.velume());
		System.out.println("The velume 2 is : " + myBox2.velume());
			
	}

}
