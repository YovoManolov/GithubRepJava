package FunctionalInterfacePackage;

import java.util.function.*;
class UseFunctionInterfaceDemo {

	public static void main(String[] args) {
		Function <Integer , Integer> factorial = (n) -> {
			int result = 1;
			for(int i = n-1 ;i >= 0 ; i--) result*=n;
			return result;
		};
      
	System.out.println("The factoriel of 9  is : "  + factorial.apply(3));	
	System.out.println("The factoriel of 9  is : "  + factorial.apply(2));		
	}

}
