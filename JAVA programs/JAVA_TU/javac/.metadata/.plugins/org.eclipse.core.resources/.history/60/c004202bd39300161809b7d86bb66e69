package Recursion;

public class Recursion {
	public static void main(String [] args){
		 int number = 5;
	       int result = factorial(number);
	       System.out.println("The factoriel is " + result);
	     }
		  
	static int result = 1;
	public static int factorial(int n){
	    if(n == 1) return n;
		return n*factorial(n-1);
	  }
}
