
class superEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       B ob = new B(5,3);
       ob.show();
	}

}

class A {
	int i;
	
}
class B extends A {
	int i;
	
	B(int i, int y ){
		super.i=i;
		i = y;
	}
	void show(){
		System.out.println("I form superclass : " + super.i);
		System.out.println("I form subclass : " + i);
	}
	
}